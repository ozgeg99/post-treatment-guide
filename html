<script>
/* Progressive enhancement marker */
document.documentElement.classList.add('js');

(function(){
  const pills  = Array.from(document.querySelectorAll('.lang-pill'));
  const panels = Array.from(document.querySelectorAll('.lang[data-lang]'));
  const status = document.getElementById('langStatus');

  const LANGS = ['en','tr','fr','ar','es'];
  const LANG_NAMES = { en:'English', tr:'TÃ¼rkÃ§e', fr:'FranÃ§ais', ar:'Ø§Ù„Ø¹Ø±Ø¨ÙŠØ©', es:'EspaÃ±ol' };
  const qs = new URLSearchParams(location.search);
  const stripLangPrefix = (id) => id.replace(/^([a-z]{2})-/, '');

  // UI translations (title + banner)
  const I18N = {
    en: { title:'Post-Treatment Care Guide', banner:'View this guide in your language' },
    tr: { title:'Tedavi SonrasÄ± BakÄ±m Rehberi', banner:'Bu rehberi kendi dilinizde gÃ¶rÃ¼ntÃ¼leyin' },
    fr: { title:'Guide de soins post-traitement', banner:'Consultez ce guide dans votre langue' },
    ar: { title:'Ø¯Ù„ÙŠÙ„ Ø§Ù„Ø¹Ù†Ø§ÙŠØ© Ø¨Ø¹Ø¯ Ø§Ù„Ø¹Ù„Ø§Ø¬', banner:'Ø§Ø¹Ø±Ø¶ Ù‡Ø°Ø§ Ø§Ù„Ø¯Ù„ÙŠÙ„ Ø¨Ù„ØºØªÙƒ' },
    es: { title:'GuÃ­a de cuidados post-tratamiento', banner:'Consulta esta guÃ­a en tu idioma' }
  };

  function applyI18n(lang){
    const dict = I18N[lang] || I18N.en;
    document.querySelectorAll('.i18n[data-key]').forEach(el=>{
      const k = el.getAttribute('data-key');
      if (dict[k]) el.textContent = dict[k];
    });
  }

  const state = { lang: null };

  function wireTOC(lang){
    const active = document.querySelector(`.lang[data-lang="${lang}"]`);
    if (!active) return;
    active.querySelectorAll('nav.toc a[href^="#"]').forEach(a => {
      const raw = a.getAttribute('href').slice(1);
      const base = stripLangPrefix(raw);
      a.setAttribute('href', `#${lang}-${base}`);
    });
  }

  function forceArabicVisibleIfSelected(){
    // Some builders inline-hide panels on mobile; beat that explicitly
    const arPanel = document.getElementById('panel-ar');
    if (!arPanel) return;

    if (state.lang === 'ar') {
      arPanel.style.display    = 'block';
      arPanel.style.visibility = 'visible';
      arPanel.style.opacity    = '1';
    } else {
      // Return control to CSS for other languages
      arPanel.style.removeProperty('display');
      arPanel.style.removeProperty('visibility');
      arPanel.style.removeProperty('opacity');
    }
  }

  function setLang(lang){
    const chosen = LANGS.includes(lang) ? lang : LANGS[0];
    state.lang = chosen;

    // Keep RTL scoped to AR panel only; do not flip <html dir>
    document.documentElement.setAttribute('lang', chosen);

    panels.forEach(p => {
      const on = p.dataset.lang === chosen;
      p.classList.toggle('active', on);
      p.setAttribute('aria-hidden', on ? 'false' : 'true');
      p.setAttribute('tabindex', on ? '0' : '-1');
    });

    // Mobile Arabic hard-fix (wins against builder rules)
    forceArabicVisibleIfSelected();

    pills.forEach(btn => {
      const on = btn.dataset.lang === chosen;
      btn.classList.toggle('active', on);
      btn.setAttribute('aria-selected', String(on));
    });

    if (status) status.textContent = `${LANG_NAMES[chosen]} selected`;

    try { localStorage.setItem('dk_lang', chosen); } catch(e){}

    const url = new URL(location.href);
    url.searchParams.set('lang', chosen);
    history.replaceState({}, '', url.toString());

    applyI18n(chosen);
    wireTOC(chosen);

    // If a hash exists, align it to the new language and scroll
    if (location.hash) {
      const raw = location.hash.slice(1);
      const base = stripLangPrefix(raw);
      const targetId = `${chosen}-${base}`;
      const el = document.getElementById(targetId);
      if (el) el.scrollIntoView({ behavior:'smooth', block:'start' });
    }
  }

  // Initial language: query > localStorage > browser > en
  const fromQuery   = (qs.get('lang') || '').toLowerCase();
  const fromStorage = (()=>{ try { return (localStorage.getItem('dk_lang')||'').toLowerCase(); } catch(e){ return '' }})();
  const fromBrowser = (navigator.language || 'en').slice(0,2).toLowerCase();
  const initial = fromQuery || fromStorage || (LANGS.includes(fromBrowser) ? fromBrowser : 'en');

  setLang(initial);
  applyI18n(initial); // ensure title/banner localized on first paint

  // Click on language pills
  pills.forEach(p => p.addEventListener('click', () => setLang(p.dataset.lang)));

  // Intercept ToC clicks to always target the active language
  document.addEventListener('click', (e)=>{
    const a = e.target.closest('nav.toc a[href^="#"]');
    if (!a) return;
    e.preventDefault();

    const raw = a.getAttribute('href').slice(1); // may be en-implants or implants
    const base = stripLangPrefix(raw);
    const targetId = `${state.lang}-${base}`;
    const el = document.getElementById(targetId);
    if (!el) return;

    a.setAttribute('href', `#${targetId}`); // keep URL copyable
    const url = new URL(location.href);
    url.hash = `#${targetId}`;
    history.replaceState({}, '', url.toString());
    el.scrollIntoView({ behavior:'smooth', block:'start' });
  });

  // Re-apply visibility when switching into/out of the mobile breakpoint
  try {
    const mq = window.matchMedia('(max-width: 900px)');
    const reapply = () => forceArabicVisibleIfSelected();
    mq.addEventListener ? mq.addEventListener('change', reapply)
                        : mq.addListener && mq.addListener(reapply);
  } catch(e){}

  // Also re-affirm after orientation changes / resizes
  let t;
  window.addEventListener('resize', () => {
    clearTimeout(t);
    t = setTimeout(() => {
      wireTOC(state.lang);
      forceArabicVisibleIfSelected();
    }, 120);
  }, { passive:true });
})();
</script>


</body>
</html>
